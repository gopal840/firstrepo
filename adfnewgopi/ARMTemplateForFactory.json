															},
															"filenamed": {
																"value": "@pipeline().parameters.pipefilename",
																"type": "Expression"
															}
														}
													}
												],
												"outputs": [
													{
														"referenceName": "AzureSqlTable3",
														"type": "DatasetReference",
														"parameters": {}
													}
												]
											}
										]
									}
								}
							]
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"parameters": {
					"pipefilename": {
						"type": "string"
					}
				},
				"annotations": [],
				"lastPublishTime": "2024-06-24T10:57:20Z"
			},
